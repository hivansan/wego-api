generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Asset {
  id              Int       @id @default(autoincrement())
  name            String?   @db.VarChar(512)
  tokenId         String    @db.VarChar(512)
  owners          String?   @db.Text
  description     String?   @db.LongText
  rariScore       Int?
  contractAddress String    @db.VarChar(512)
  owner           String?   @db.Text
  animationUrl    String?   @db.VarChar(512)
  traits          String?   @db.Text
  createdAt       DateTime? @db.DateTime(0)
  updatedAt       DateTime? @db.DateTime(0)
  slug            String    @db.VarChar(512)
  tokenMetadata   String?   @db.Text
  imgBig          String?   @db.VarChar(512)
  imgSmall        String?   @db.VarChar(512)
}

model AssetTrait {
  id              Int     @id @default(autoincrement())
  contractAddress String? @db.VarChar(42)
  tokenId         String? @db.VarChar(512)
  traitType       String? @db.VarChar(512)
  value           String? @db.VarChar(512)
}

model Collection {
  id                    Int       @id @default(autoincrement())
  slug                  String?   @db.VarChar(512)
  name                  String?   @db.VarChar(512)
  floorPrice            String?   @db.VarChar(512)
  wegoScore             String?   @db.VarChar(512)
  releaseDate           DateTime? @db.DateTime(0)
  released              Boolean?
  contractAddress       String?   @db.VarChar(512)
  imgPortrait           String?   @db.VarChar(512)
  imgMain               String?   @db.VarChar(512)
  featuredCollection    Boolean?
  totalSupply           String?   @db.VarChar(512)
  osData                String?   @db.Text
  oneDayVolume          String?   @db.VarChar(30)
  oneDayChange          String?   @db.VarChar(30)
  oneDaySales           String?   @db.VarChar(30)
  oneDayAveragePrice    String?   @db.VarChar(30)
  sevenDayVolume        String?   @db.VarChar(30)
  sevenDayChange        String?   @db.VarChar(30)
  sevenDaySales         String?   @db.VarChar(30)
  sevenDayAveragePrice  String?   @db.VarChar(30)
  thirtyDayVolume       String?   @db.VarChar(30)
  thirtyDayChange       String?   @db.VarChar(30)
  thirtyDaySales        String?   @db.VarChar(30)
  thirtyDayAveragePrice String?   @db.VarChar(30)
  totalVolume           String?   @db.VarChar(30)
  totalSales            String?   @db.VarChar(30)
  count                 String?   @db.VarChar(30)
  numOwners             String?   @db.VarChar(30)
  averagePrice          String?   @db.VarChar(20)
  numReports            String?   @db.VarChar(10)
  marketCap             String?   @db.VarChar(40)
  createdAt             DateTime? @db.DateTime(0)
  updatedAt             DateTime? @db.DateTime(0)
  featuredScore         Int?
  description           String?   @db.Text
  imgLarge              String?   @db.VarChar(512)
  tags                  String?   @db.Text
  twitter               String?   @db.VarChar(40)
  discord               String?   @db.VarChar(40)
  instagram             String?   @db.VarChar(40)
  telegram              String?   @db.VarChar(40)
  website               String?   @db.VarChar(40)
}

model ContractAbi {
  id          Int      @id @default(autoincrement())
  address     String?  @unique(map: "address") @db.VarChar(255)
  abi         String?  @db.Text
  isJSON      Boolean?
  totalSupply BigInt?
  tokenUri    String?  @db.VarChar(512)
  abiMethod   String?  @db.VarChar(512)
}

model Trait {
  slug        String    @db.VarChar(512)
  traitType   String    @db.VarChar(100)
  value       String    @db.VarChar(100)
  displayType String?   @db.VarChar(512)
  maxValue    String?   @db.VarChar(512)
  traitCount  Int?
  order       Int?
  createdAt   DateTime? @db.DateTime(0)
  updatedAt   DateTime? @db.DateTime(0)

  @@id([slug, traitType, value])
  @@unique([slug, traitType, value], map: "productid_keyword_index")
}

model nft {
  id          Int     @id @default(autoincrement())
  name        String? @db.VarChar(512)
  contract    String  @db.VarChar(512)
  tokenId     Int
  owners      String? @db.Text
  description String? @db.Text
  imageBig    String  @db.VarChar(512)
  imageSmall  String? @db.VarChar(512)
  properties  String? @db.Text
  rariscore   Int?
}
