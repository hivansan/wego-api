{
  "name": "Asset",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "contractAddress": {
      "type": "string",
      "required": true
    },
    "tokenId": {
      "type": "number",
      "required": true
    },
    "owners": {
      "type": "object"
    },
    "owner": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "imageBig": {
      "type": "string"
    },
    "animationUrl": {
      "type": "string"
    },
    "imageSmall": {
      "type": "string"
    },
    "traits": {
      "type": [
        "object"
      ]
    },
    "rariScore": {
      "type": "number"
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
