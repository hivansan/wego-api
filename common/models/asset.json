{
  "name": "Asset",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "contract": {
      "type": "string",
      "required": true,
      "description": "contract's address"
    },
    "tokenId": {
      "type": "number",
      "required": true
    },
    "owners": {
      "type": [
        "string"
      ],
      "required": false
    },
    "description": {
      "type": "text",
      "required": false
    },
    "imageBig": {
      "type": "string",
      "required": false
    },
    "imageSmall": {
      "type": "string"
    },
    "properties": {
      "type": [
        "object"
      ]
    },
    "rariScore": {
      "type": "number"
    },
    "createdAt": {
      "type": "date",
      "default": "$now"
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
